<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.OrderItemDao">
    <insert id="add" parameterType="com.entity.OrderItem">
        insert into orderitem(buyOrderID,sellOrderID,finalPrice,quantity) values(#{buyOrderID},#{sellOrderID},#{finalPrice},#{quantity})
    </insert>
</mapper>